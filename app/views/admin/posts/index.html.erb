      <h2>posts</h2>
       <table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>
            </th>
            <th></th>
            <th></th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <% @posts.each do |post| %>
        <tbody>
          <tr>
            <td> <%= post.created_at.strftime('%Y/%m/%d') %><%= post.member.name %><br>
            <%= post.title %><br></td>
            <td></td>
            <td></td>
            <td><% if post.status == false %>
	  	      <h7><strong class='ture' style="color :limegreen;">表示</strong></h7>
	  	      <% else %>
	  	      <h7 ><strong class='false' style="color :#d3d3d3;">非表示</strong></h7>
	  	      <% end %>
	  	      </td>
            <td><%= link_to "詳細", admin_post_path(post.id), class:"btn btn-success"%></td>

          </tr>
        <% end %>
        </tbody>
      </table>