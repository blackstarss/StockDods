<div class="container mt-5 ">
  <div class="row">
    <div class= "col-6 m-0 p-0 mx-auto">
      <h2>メンバー</h2>
      <table class="table table-hover table-inverse">
        <% @members.each do |member| %>
        <tbody>
	  	   	<tr>
            <td>
              <%= member.created_at.strftime("%Y年%m月%d日%H時%M分") %>に新規登録<br>
              <%= link_to admin_member_path(member) do %>
              <%= member.name %><br>
              <% end %>
              投稿数：<%= Post.count %><br>
            </td>

            <!--投稿数-->

	  	  	</tr>
  	  	  <% end %>
  	  	</tbody>
      </table>
    </div>
  </div>
</div>