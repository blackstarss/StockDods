<%= form_with model:@post, url:'/posts', local:true do |f| %>
　　　<div class="form-group row">
　　　  <label>タイトル</label>
　　　  <%= f.text_field :tilte, placeholder: "タイトル", class:"form-control w-25 " %>
　　　</div>
　　　<div class="form-group row">
　　　  <label>ジャンル</label>
　　　  <%= f.text_field :genre, placeholder: "ジャンル", class:"form-control w-25 " %>
　　　</div>
　　　<div class="form-group row">
　　　  <label>本文</label>
　　　  <%= f.text_area :article, placeholder: "＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃", 'data-provider': :summernote %>
　　　</div>
　　　<div class="form-group row">
　　　  <label>引用記事リンク</label>
　　　  <%= f.text_field :link, placeholder: "リンク", class:"form-control w-25 " %>
　　　</div>
　　　<div class="form-group-row">
　　　  <%= f.submit '新規登録', class: "btn btn-success" %>
　　　</div>
　　　<% end %>